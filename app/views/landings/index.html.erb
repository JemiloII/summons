 <div class= "email">
 
      <div class="lineup">
      <%= button_to "Get Notified!", landings_path, method: :post, class: "signup-submit button" %>
      </div>
      <div class="lineup">
      <%= text_field_tag(:collect_emails, 'Type your email here...', size: 30, class: "signup-email", "onfocus" => "if(this.value == 'Type your email here...') {this.value=''}", "onblur" => "if(this.value == ''){this.value ='Type your email here...'}") %>
      </div>
 </div>
